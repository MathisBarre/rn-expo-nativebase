{
  "expo": {
    "name": "rn-expo-nativebase",
    "slug": "rn-expo-nativebase",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": ".src//icon.png",
    "splash": {
      "image": ".src//splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": ".src//adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": ".src//favicon.png"
    }
  }
}
